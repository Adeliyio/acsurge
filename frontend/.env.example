# AdCopySurge Frontend Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_APP_NAME=AdCopySurge

# Paddle Configuration (Replace with your actual Paddle credentials)
REACT_APP_PADDLE_VENDOR_ID=your-paddle-vendor-id
REACT_APP_PADDLE_ENVIRONMENT=sandbox

# Supabase Configuration (if still using)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key

# Environment
NODE_ENV=development

# Feature Flags
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_COMPETITOR_ANALYSIS=true
